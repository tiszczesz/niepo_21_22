@model IEnumerable<Book>
@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <h1>Lista książek</h1>
    <table class="table table-striped table-bordered">
        <tr>
            <th>
                Lp
            </th>
            <th>
                Tytuł
            </th>
            <th>
                Autor
            </th>
            <th>
                Cena
            </th>
            <th>
                Opis
            </th>
            <th>

            </th>
        </tr>
        @{ var i = 0; }
        @foreach (var book in Model) {
            i++;
            <tr>
                <td>@i</td>
                <td>@book.Title</td>
                @*<td>@Html.DisplayFor(modelItem=>book.Title)</td>*@
                <td>@Html.DisplayFor(modelItem => book.Author)</td>
                <td>@Html.DisplayFor(modelItem => book.Price)</td>
                <td>@Html.DisplayFor(modelItem => book.Description)</td>
                <td>

                </td>
            </tr>
        }
    </table>
</div>